{
  "name": "ngx-dotlottie-web",
  "description": "TypeScript NPM Module Boilerplate",
  "version": "0.0.0-beta12",
  "author": "Victor Enogwe",
  "bugs": "https://github.com/victor-enogwe/ngx-dotlottie-web/issues",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "feat",
          "fix",
          "docs",
          "chore",
          "style",
          "refactor",
          "ci",
          "test",
          "perf",
          "revert",
          "vercel"
        ]
      ]
    }
  },
  "depcheck": {
    "ignoreBinPackage": false,
    "ignores": [
      "@angular-devkit/build-angular",
      "@angular/build",
      "@angular-builders/jest",
      "@angular/compiler-cli",
      "@angular/platform-browser-dynamic",
      "@commitlint/cli",
      "@commitlint/config-conventional",
      "@cypress/schematic",
      "@lottiefiles/dotlottie-web",
      "@types/node",
      "@jest/globals",
      "ng-packagr",
      "jest",
      "tslib"
    ],
    "ignorePatterns": [
      "dist",
      "coverage"
    ]
  },
  "dependencies": {
    "@angular-builders/jest": "^19.0.0",
    "@angular/animations": "19.1.5",
    "@angular/cdk": "19.1.3",
    "@angular/common": "19.1.5",
    "@angular/core": "19.1.5",
    "@angular/forms": "19.1.5",
    "@angular/material": "19.1.3",
    "@angular/platform-browser": "19.1.5",
    "@angular/platform-browser-dynamic": "19.1.5",
    "@angular/platform-server": "19.1.5",
    "@angular/router": "19.1.5",
    "@angular/ssr": "19.1.5",
    "angular-cli-ghpages": "2.0.3",
    "rxjs": "^7.8.1",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.1.5",
    "@angular-eslint/eslint-plugin": "19.0.2",
    "@angular-eslint/eslint-plugin-template": "19.0.2",
    "@angular-eslint/template-parser": "19.0.2",
    "@angular/animations": "19.1.5",
    "@angular/build": "19.1.5",
    "@angular/cli": "19.1.5",
    "@angular/compiler": "19.1.5",
    "@angular/compiler-cli": "19.1.5",
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "@cypress/schematic": "3.0.0",
    "@eslint/compat": "1.2.6",
    "@eslint/js": "9.20.0",
    "@eslint/markdown": "6.2.2",
    "@jest/globals": "29.7.0",
    "@lottiefiles/dotlottie-web": "0.40.1",
    "@types/express": "4.17.17",
    "@types/jest": "^29.5.14",
    "@types/node": "22.13.1",
    "@typescript-eslint/eslint-plugin": "8.23.0",
    "canvas": "^3.1.0",
    "depcheck": "1.4.7",
    "eslint": "9.20.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-json": "4.0.1",
    "eslint-plugin-prettier": "5.2.3",
    "express": "4.21.2",
    "globals": "15.14.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ng-packagr": "19.1.2",
    "prettier": "3.4.2",
    "syncpack": "13.0.2",
    "typescript": "5.7.3",
    "typescript-eslint": "8.23.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "ngx-dotlottie-web/**/*"
  ],
  "homepage": "https://github.com/victor-enogwe/ngx-dotlottie-web#readme",
  "keywords": [
    "boilerplate",
    "module",
    "npm",
    "typescript"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "packageManager": "pnpm@9.15.4",
  "pnpm": {
    "overrides": {
      "@typescript-eslint/utils": "8.23.0",
      "glob": "11.0.1",
      "rimraf": "6.0.1",
      "gh-pages": "6.3.0"
    }
  },
  "private": false,
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": "victor-enogwe/ngx-dotlottie-web.git",
  "scripts": {
    "build": "ng build ngx-dotlottie-web",
    "build:site": "ng build ngx-dotlottie-demo",
    "commitlint": "commitlint",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "depcheck": "depcheck --quiet",
    "e2e": "ng e2e",
    "format": "prettier --log-level error --write --ignore-unknown --cache-location node_modules/.cache .",
    "lint": "eslint --cache --cache-location node_modules/.cache --fix .",
    "ng": "ng",
    "postinstall": "syncpack fix-mismatches",
    "postversion": "git push && git push --tags",
    "preinstall": "syncpack format",
    "prepare": "pnpm build",
    "prepublishOnly": "npm test && pnpm lint",
    "preversion": "pnpm lint",
    "publish:ngx-dotlottie-web": "pnpm publish dist/ngx-dotlottie-web --access public --provenance --no-git-checks",
    "serve:ssr:ngx-dotlottie-demo": "node dist/ngx-dotlottie-demo/server/server.mjs",
    "start": "ng serve",
    "test": "ng test --project ngx-dotlottie-web",
    "test:e2e": "ng e2e --project ngx-dotlottie-demo",
    "tsc": "tsc --noEmit",
    "update:deps": "syncpack update",
    "use": "pnpm --filter",
    "version": "pnpm format && git add -A src",
    "watch": "ng build --watch --configuration development"
  },
  "syncpack": {
    "sortPackages": true,
    "formatBugs": true,
    "formatRepository": true,
    "customTypes": {
      "engines": {
        "path": "engines",
        "strategy": "versionsByName"
      }
    },
    "source": [
      "package.json",
      "ngx-dotlottie-web/package.json"
    ],
    "sortAz": [
      "bin",
      "contributors",
      "dependencies",
      "devDependencies",
      "keywords",
      "peerDependencies",
      "resolutions",
      "scripts"
    ],
    "sortExports": [
      "types",
      "node-addons",
      "node",
      "browser",
      "import",
      "require",
      "development",
      "production",
      "default"
    ],
    "sortFirst": [
      "name",
      "description",
      "version",
      "author"
    ],
    "versionGroups": [
      {
        "dependencies": [
          "**"
        ],
        "dependencyTypes": [
          "dev",
          "peer",
          "prod"
        ],
        "packages": [
          "**"
        ],
        "policy": "sameRange",
        "preferVersion": "highestSemver"
      },
      {
        "label": "Versions in engines field should all match",
        "dependencyTypes": [
          "engines"
        ],
        "dependencies": [
          "**"
        ],
        "packages": [
          "**"
        ]
      },
      {
        "dependencies": [
          "@types/**"
        ],
        "dependencyTypes": [
          "!dev"
        ],
        "isBanned": true,
        "label": "@types packages should only be under devDependencies"
      }
    ]
  },
  "types": "lib/index.d.ts"
}
